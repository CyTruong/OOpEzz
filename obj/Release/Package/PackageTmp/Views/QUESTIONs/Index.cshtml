@model IEnumerable<OE.Models.QUESTION>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.QUESTION_TEXT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.QUESTION_USER_FK)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BEST_ANSWER_FK)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.STATUS)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.QUESTION_TEXT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.QUESTION_USER_FK)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BEST_ANSWER_FK)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.STATUS)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PK_SEQ }) |
            @Html.ActionLink("Details", "Details", new { id=item.PK_SEQ }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PK_SEQ })
        </td>
    </tr>
}

</table>
