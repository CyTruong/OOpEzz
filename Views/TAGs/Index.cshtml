@model IEnumerable<OE.Models.TAG>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TAG_NAME)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TAG_NAME)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PK_SEQ }) |
            @Html.ActionLink("Details", "Details", new { id=item.PK_SEQ }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PK_SEQ })
        </td>
    </tr>
}

</table>
